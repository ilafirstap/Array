#include <iostream>
using namespace std;

int main ()
{
    //Mendeklarasikan array A dengan 5 buah elemen bertipe integer

    int n;

    cout<<"Masukkan jumlah bilangan :"<<endl;
    cin>>n;
    int Angka[n];
    for (int C=0; C<n; C++) {
        cout<<"Angka["<<C<<"] = "; cin>>Angka[C];
    }

    cout << "Nilai Array adalah :" <<endl;
    for (int C=0; C<n; C++){
    cout<<"["<<Angka[C]<<"]";}
    return 0;
}
